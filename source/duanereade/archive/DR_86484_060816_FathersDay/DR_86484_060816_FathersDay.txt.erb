<% require 'active_support/all' %>
<% instance_variable_set("@base_data_file", data.duanereade.DR_86484_060816_FathersDay.DR_86484_060816_FathersDay) %>
<% @base_data_file.each{|variable, value| instance_variable_set("@base_#{variable}", value)} %>

<%= @base_pre_header_text %>

<% if @base_hero_img_url!=''%>
<%=@base_hero_img_alt%>
<%=@base_hero_img_link%>

<% end %>

<% @base_data_file.articles.each do |f| %>
<% if f["type"]=="product-right-image" || f["type"]=="product-left-image" || f["type"]=="story-image" || f["type"]=="story-image-no-text"  || f["type"]=="story-image-center-text" %>
<%= f["text_img_alt"] %>
<%= f["promo_btn_url"] %>

<% end %>

<% if f["type"]== "image" %>
<%= f["img_alt"] %>
<%= f["url"] %>

<% end %>
<% end %>

<% if @base_callout_img_alt!='' %>
<%= @base_callout_img_alt %>
<%=@base_callout_img_link%>

<% end %>

<% @base_data_file.footernav.each do |f| %>
<%= f["text"] %>
<%= f["link"] %>

<% end %>

<% @base_data_file.socialnav.each do |f| %>
<%= f["alt"] %>
<%= f["link"] %>

<% end %>

<%=@base_unsubscribe_text%>
<%=@base_unsubscribe_link%>

<%=@base_privacy_text%>
<%=@base_privacy_link%>

<%=@base_copyright%>
