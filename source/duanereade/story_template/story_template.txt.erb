<% require 'active_support/all' %>
<% data.duanereade.dr_042116_Pharmacy.dr_042116_Pharmacy.each{|variable, value| instance_variable_set("@base_#{variable}", value)} %>

<%= @base_pre_header_text %>

<%=@base_hero_img_alt%>
<%=@base_hero_img_link%>

<% data.duanereade.dr_042116_Pharmacy.dr_042116_Pharmacy.articles.each do |f| %>
<% if f["type"]=="product-right-image" || f["type"]=="product-left-image" %>
<%= f["text_img_alt"] %>
<%= f["promo_btn_url"] %>

<% end %>

<% if f["type"]== "image" %>
<%= f["img_alt"] %>
<%= f["url"] %>

<% end %>
<% end %>

<%= @base_callout_img_alt %>
<%=@base_callout_img_link%>

<% data.duanereade.story_template.story_template.footernav.each do |f| %>
<%= f["text"] %>
<%= f["link"] %>

<% end %>

<% data.duanereade.story_template.story_template.socialnav.each do |f| %>
<%= f["alt"] %>
<%= f["link"] %>

<% end %>

<%=@base_unsubscribe_text%>
<%=@base_unsubscribe_link%>

<%=@base_privacy_text%>
<%=@base_privacy_link%>

<%=@base_copyright%>
