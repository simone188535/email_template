	<% require 'active_support/all' %>
<% instance_variable_set("@base_data_file", data.otp.OTP_080417_Email.OTP_080417_Email) %>
<% @base_data_file.each{|variable, value| instance_variable_set("@base_#{variable}", value)} %>
<style type="text/css">
    /* This style block will be inlined when running 'grunt build' and stripped from the header */

    table td {
        border-collapse: collapse;
		border-spacing: 0; 
		mso-table-lspace: 0pt; 
		mso-table-rspace: 0pt; 
		text-align: left;
    }
    img {
        display: block;
        margin: 0;
    }
	.footer {
		font: 10pt arial, sans-serif; 
		color: #666666;
		text-align: center;
	}
	a.footer {
		font: 10pt arial, sans-serif; 
		color: #888888;
	}
	.topLink {
		color: #888888; 
		text-decoration: underline;
	}
	.topSection, td.preheader {
		color: #2a2a2a; 
		font-family: Arial, Helvetica, sans-serif; 
		font-weight: normal; 
		font-size: 12px; 
		line-height: 16px; 
		border-collapse: collapse;
	}
	.topPreHeader {
		text-align: left; 
	}
	.topViewWebPage {			
		text-align: right;
	}	

</style>
<table border="0" width="100%" cellpadding="0" cellspacing="0" align="center">
	<tr><td align="center">
		<table border="0" width="600" cellpadding="0" cellspacing="0" align="center">
			<tr> 
				<td align="center"><font style="FONT-SIZE: 10px; COLOR: #888888; LINE-HEIGHT: 14px; FONT-FAMILY: Arial, Verdana, Helvetica, sans-serif">&nbsp;</font></td>
			</tr>
			<tr style="text-align: left;">
				<td class="preheader">
					<!--[if mso]><table border="0" cellpadding="0"  cellspacing="0" width="100%"><tr><td width="50%" valign="top"><![endif]-->
					<table width="300" border="0" cellpadding="0" cellspacing="0" align="left" class="force-row topSection">
						<tr class="topPreHeader">
							<td class="col column1 topSection topPreHeader" valign="top">
								<a href="<%= @base_preheader_link %>" class="topLink" target="_blank"><%= @base_preheader_text %></a>
								<br class="hideOnMob">
								<br class="hideOnMob">
							</td>
						</tr>
					</table>
					<!--[if mso]></td><td width="50%" valign="top"><![endif]-->
					<table width="300" border="0" cellpadding="0" cellspacing="0" align="right" class="force-row topSection">
						<tr class="topViewWebPage">
							<td class="col column2 topSection topViewWebPage" valign="top">
								<a href="<%= @base_viewaswebpage_link %>" class="topLink" target="_blank"><%= @base_viewaswebpage_text %></a>
								<br class="hideOnMob">
								<br class="hideOnMob">
							</td>
						</tr>
					</table>
					<!--[if mso]></td></tr></table><![endif]-->
				</td>
			</tr>
			<tr><td align="center">
				<!-- Header -->
				<table border="0" width="600" cellpadding="0" cellspacing="0" align="center">
				    <tr>
				        <td>
				            <a href="<%=@base_logo_img_link%>">
				                <img src="<%=@base_logo_img_url%>" alt="<%=@base_logo_img_alt%>" title="<%=@base_logo_img_alt%>" border="0">
				            </a>
				        </td>

				         <td>
				            <a href="<%=@base_products_img_link%>">
				                <img src="<%=@base_products_img_url%>" alt="<%=@base_products_img_alt%>" title="<%=@base_products_img_alt%>" border="0">
				            </a>
				        </td>

				        <td>
				            <a href="<%=@base_locations_img_link%>">
				                <img src="<%=@base_locations_img_url%>" alt="<%=@base_locations_img_alt%>" title="<%=@base_locations_img_alt%>" border="0">
				            </a>
				        </td>

				        <td>
				            <a href="<%=@base_inspirations_img_link%>">
